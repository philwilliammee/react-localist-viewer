{
  "name": "react-localist-viewer",
  "version": "0.3.0",
  "description": "React component - Uses localist api to view events",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/philwilliammee/react-localist-viewer"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-examples": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "jest": "jest",
    "coverage": "jest --coverage",
    "build": "rimraf dist && cross-env NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^9.1.2",
    "cross-env": "^6.0.3",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.22.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@reach/dialog": "^0.11.2",
    "axios": "^0.19.2",
    "build-url": "^1.3.3",
    "moment": "^2.27.0",
    "node-sass": "^4.14.1",
    "react-big-calendar": "^0.27.0",
    "react-paginate": "^6.4.0",
    "react-spring": "^8.0.27",
    "truncate": "^2.1.0",
    "truncate-url": "^1.0.0"
  },
  "keywords": [
    "localist",
    "events",
    "cwd",
    "cwd_events",
    "react"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
